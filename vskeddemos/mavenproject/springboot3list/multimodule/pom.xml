<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.vsked</groupId>
    <artifactId>multimodule</artifactId>
    <version>1.0</version>

    <name>multimodule</name>
    <description>multimodule written by vsked</description>
    <packaging>pom</packaging>

    <modules>
        <module>testlog</module>
        <module>domain</module>
        <module>domaininterface</module>
        <module>common</module>
        <module>spring</module>
        <module>mybatisplus</module>
        <module>dataredis</module>
        <module>datajpa</module>
        <module>service</module>
        <module>webapi</module>
        <module>webapp</module>
    </modules>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <maven.compiler.encoding>UTF-8</maven.compiler.encoding>

        <java.version>21</java.version>
        <maven.compiler.source>21</maven.compiler.source>
        <maven.compiler.target>21</maven.compiler.target>
        <maven.deploy.skip>true</maven.deploy.skip>
        <maven.install.skip>true</maven.install.skip>

        <mavenCompilerPlugin.version>3.14.0</mavenCompilerPlugin.version>
        <mavenSurefirePlugin.version>3.5.3</mavenSurefirePlugin.version>

        <global.version>1.0</global.version>

        <mysqlConnector.version>9.4.0</mysqlConnector.version>

        <slf4j.version>2.0.17</slf4j.version>
        <log4j.version>2.25.1</log4j.version>
        <testng.version>7.11.0</testng.version>
        <springboot.version>3.5.4</springboot.version>

        <mybatisPlusStart.version>3.5.12</mybatisPlusStart.version>
        <mybatisSrping.version>3.0.5</mybatisSrping.version>

    </properties>


</project>